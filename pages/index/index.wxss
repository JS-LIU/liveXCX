@import "../template/courseNameTemp";
@import "../template/courseTimeTemp.wxss";
@import "../template/teacherTemp.wxss";
.wrap{
  position:fixed;
  top:0;
  left:0;
  width:100%;
  height:100%;
  background:#FAFAFA;
}
.content{
  position:fixed;
  width:100%;
  height:100%;
  top:0;
  left:0;
  z-index:1;
}
.owned_course_list{
  margin-top:48rpx;
  width:100%;
  height:100%;
}
.owned_course{
  display:flex;
  flex-direction: column;
  width:690rpx;
  height:300rpx;
  margin:0 auto 32rpx;
  background: #FFF;
  box-shadow:0rpx 6rpx 20rpx 0rpx rgba(0,0,0,0.08);
  border-radius: 8rpx;
}
