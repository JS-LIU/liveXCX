@import "../template/courseNameTemp";
@import "../template/courseTimeTemp.wxss";
.wrap{
  position: fixed;
  top:0;
  left:0;
  width:100%;
  height:100%;
  background:#FAFAFA;
  z-index: 1;
}
.order_detail{
  position:relative;
  z-index: 2;
}
.order_status{
  font-size:40rpx;
  color:#000000;
  padding:30rpx;
}
.course_info{
  height:176rpx;
  background:#FFFFFF;
  display:flex;
  flex-direction: column;
}
.pay_info_title{
  font-size:28rpx;
  color:#000000;
  margin:46rpx 0 20rpx 30rpx;
}
.pay_info{
  background:#FFFFFF;
  display:flex;
  flex-direction: column;
  justify-content: space-between;
  padding:30rpx 58rpx;
  height: 184rpx;
}
.pay_info_line{
  display:flex;
  flex-direction: row;
  justify-content: space-between;
  font-size:24rpx;
}